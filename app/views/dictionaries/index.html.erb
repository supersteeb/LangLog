<p id="notice"><%= notice %></p>

<h1>Dictionaries</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Chinese</th>
      <th>English</th>
      <th>Pinyin</th>
      <th>Part of speech</th>
      <th>Times tested</th>
      <th>Times correct</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dictionaries.each do |dictionary| %>
      <tr>
        <td><%= dictionary.user %></td>
        <td><%= dictionary.chinese %></td>
        <td><%= dictionary.english %></td>
        <td><%= dictionary.pinyin %></td>
        <td><%= dictionary.part_of_speech %></td>
        <td><%= dictionary.times_tested %></td>
        <td><%= dictionary.times_correct %></td>
        <td><%= link_to 'Show', dictionary %></td>
        <td><%= link_to 'Edit', edit_dictionary_path(dictionary) %></td>
        <td><%= link_to 'Destroy', dictionary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dictionary', new_dictionary_path %>
